[push]
	default = simple
[alias]
	st = status
	ci = commit -v
	co = checkout
	br = branch
	unstage = reset HEAD --
	last = log -1 HEAD
	pom = push origin master
	pum = pull origin master
	pod = push origin dev
	pud = pull origin dev
	pop = push origin prod
	pup = pull origin prod
[diff]
	tool = vimdiff
[difftool]
    prompt = false
[core]
    include = /Users/aline/.gitconfig_credentials
