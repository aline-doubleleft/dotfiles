[include]
    path = /Users/alinefreitas/.gitconfig_credentials
[push]
	default = simple
[alias]
	st = status -sb
	ci = commit -v
	co = checkout
	br = branch
	unstage = reset HEAD --
	last = log -1 HEAD
	pom = push origin master
	pum = pull origin master
	pod = push origin dev
	pud = pull origin dev
	pop = push origin prod
	pup = pull origin prod
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[diff]
	tool = vimdiff
[difftool]
	prompt = false


[color]
	ui = auto
[core]
	pager = less
